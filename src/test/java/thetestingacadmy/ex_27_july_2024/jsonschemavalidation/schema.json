{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "userId",
        "id",
        "title",
        "body"
    ],
    "properties": {
        "userId": {
            "type": "integer",
            "default": 0,
            "title": "The userId Schema",
            "examples": [
                1
            ]
        },
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                2
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "que est esse"
            ]
        },
        "body": {
            "type": "string",
            "default": "",
            "title": "The body Schema",
            "examples": [
                "there are something that need to change "
            ]
        }
    },
    "examples": [{
        "userId": 1,
        "id": 2,
        "title": "que est esse",
        "body": "there are something that need to change "
    }]
}